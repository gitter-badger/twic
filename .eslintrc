{
	"ecmaFeatures": {
		"modules": true,
		"classes": true,
		"forOf": true,
		"blockBindings": true,
		"arrowFunctions": true,
		"templateStrings": true,
		"jsx": true,
		"superInFunctions": true,
		"destructing": true,
		"defaultParams": true
	},
	"env": {
		"browser": true
	},
	"rules": {
		"quotes": [2, "single"],
		"strict": 0,
		"no-console": 0,
		"no-unused-vars": 0,
		"yoda": 0,
		"no-shadow": 0,
		"dot-notation": 0,
		"no-eq-null": 2,
		"brace-style": 2,
		"no-multiple-empty-lines": 2,
		"space-after-keywords": 2
	}
}
