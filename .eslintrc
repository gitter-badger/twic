{
	"env": {
		"es6": true
	},
	"rules": {
		"quotes": [2, "single"],
		"strict": [2, "never"],
		"no-console": 0,
		"no-unused-vars": 0,
		"yoda": 0,
		"no-shadow": 0,
		"dot-notation": 0,
		"no-eq-null": 2,
		"brace-style": 2,
		"no-multiple-empty-lines": 2,
		"space-after-keywords": 2,
		"no-mixed-spaces-and-tabs": 2,
		"curly": 2,
		"eqeqeq": 2,
		"no-extra-bind": 2,
		"indent": [2, "tab"],
		"one-var": [2, {
			"initialized": "never"
		}],
		"no-unreachable": 1,
		"semi": [2, "always"]
	}
}
